@article{steinMechanisticUnderstandingRole2017a,
  title = {Mechanistic {{Understanding}} of the {{Role}} of {{Evaporation}} in {{Electrode Processing}}},
  author = {Stein, Malcolm and Mistry, Aashutosh and Mukherjee, Partha P.},
  date = {2017},
  journaltitle = {J. Electrochem. Soc.},
  volume = {164},
