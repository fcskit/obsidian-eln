@article{pohlOxidationStateLocal2014,
  title = {Oxidation State and Local Structure of a High-Capacity {{LiF}}/{{Fe}}({{V2O5}}) Conversion Cathode for {{Li-ion}} Batteries},
  author = {Pohl, A.H. and Guda, A.A. and Shapovalov, V.V. and Witte, R. and Das, B. and Scheiba, F. and Rothe, J. and Soldatov, A.V. and Fichtner, M.},
