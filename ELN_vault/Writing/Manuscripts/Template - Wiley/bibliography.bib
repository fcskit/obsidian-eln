@article{labriniDelithiatedLiyCo08Ni02016,
  title = {Delithiated {{LiyCo0}}.{{8Ni0}}.{{1Mn0}}.{{1O2}} Cathode Materials for Lithium-Ion Batteries: {{Structural}}, Magnetic and Electrochemical Studies},
