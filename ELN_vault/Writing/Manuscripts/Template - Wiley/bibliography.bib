@article{jaugstetterElectrochemistryConfinement2022,
  title = {Electrochemistry under Confinement},
  author = {Jaugstetter, Maximilian and Blanc, Niclas and Kratz, Markus and Tschulik, Kristina},
  date = {2022-03-11},
  journaltitle = {Chem. Soc. Rev.},
  volume = {51},
