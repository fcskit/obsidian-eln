  author = {Yu, Xiaochen and Yu, Baojun and Zhang, Jie and Zhang, Yang and Zeng, Jingxuan and Chen, Mingming and Wang, Chengyang},
  date = {2018-09-07},
  journaltitle = {ChemistrySelect},
