@article{kizzireLocalStructureAnalysis2022,
  title = {Local {{Structure Analysis}} and {{Modelling}} of {{Lignin-Based Carbon Composites}} through the {{Hierarchical Decomposition}} of the {{Radial Distribution Function}}},
  author = {Kizzire, Dayton G. and Garcia-Negron, Valerie and Harper, David P. and Keffer, David J.},
  date = {2022-02},
  journaltitle = {ChemistryOpen},
