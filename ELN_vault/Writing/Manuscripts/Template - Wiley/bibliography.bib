  author = {Gomez, Iñaki and Mecerreyes, David and Blazquez, J. Alberto and Leonet, Olatz and Ben Youcef, Hicham and Li, Chunmei and Gómez-Cámer, Juan Luis and Bondarchuk, Oleksandr and Rodriguez-Martinez, Lide},
