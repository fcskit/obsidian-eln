@article{daemiVisualizingCarbonBinder2018,
  title = {Visualizing the {{Carbon Binder Phase}} of {{Battery Electrodes}} in {{Three Dimensions}}},
  author = {Daemi, Sohrab R. and Tan, Chun and Volkenandt, Tobias and Cooper, Samuel J. and Palacios-Padros, Anna and Cookson, James and Brett, Dan J.L. and Shearing, Paul R.},
  date = {2018},
  journaltitle = {ACS Appl. Energy Mater.},
  volume = {1},
