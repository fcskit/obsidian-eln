@article{chenLigninderivedNitrogendopedPorous2020,
  title = {Lignin-Derived Nitrogen-Doped Porous Ultrathin Layered Carbon as a High-Rate Anode Material for Sodium-Ion Batteries},
  author = {Chen, Suli and Feng, Fan and Ma, Zi-Feng},
